Function AdvScreen::hideMessage(Reference this, Integer fFlush, Integer fWait)
	CALL isSelect
	PUSH_VAL Integer -1i
	==
	JUMP_OK _l_7
	PUSH_VAL Integer 256i
	CALL transparencySelect
	NOP
@_l_7:
	PUSH_FIELD msgFrame
	PUSH_VAR fFlush
	PUSH_VAR fWait
	CALL_METHOD hide
	NOP
	PUSH_VAL Integer 0i
	Return
EndFunc

