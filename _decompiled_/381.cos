Function AdvScreen::setCg(Reference this, String str, Integer x, Integer y, Integer w, Integer h)
	PUSH_VAL String "CG-"
	PUSH_VAR str
	+
	CALL Debug
	NOP
	PUSH_VAR str
	PUSH_FIELD cg
	CALL CheckSetupCg
	NOP
	PUSH_FIELD cg
	GET_FIELD pt
	PUSH_VAR x
	PUSH_VAR y
	CALL IPoint
	POP =
	NOP
	PUSH_FIELD fSetCg
	PUSH_VAL Integer -1i
	POP =
	NOP
	PUSH_FIELD fSetCgRGB
	PUSH_VAL Integer 0i
	POP =
	NOP
	PUSH_FIELD fUpdate
	PUSH_VAL Integer -1i
	POP =
	NOP
	PUSH_VAL Integer 0i
	Return
EndFunc

