Function AdvScreen::showMessage(Reference this, Integer fFlush, Integer fHide)
	CALL isSelect
	PUSH_VAL Integer -1i
	==
	JUMP_OK _l_7
	PUSH_VAL Integer 0i
	CALL transparencySelect
	NOP
@_l_7:
	PUSH_FIELD msgFrame
	PUSH_VAR fFlush
	PUSH_VAR fHide
	CALL_METHOD show
	NOP
	PUSH_VAL Integer 0i
	Return
EndFunc

