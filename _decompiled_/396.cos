Function Appreciation()
	CALL StopBgm
	NOP
	CALL EnterRecollectMode
	NOP
	INIT_LOCAL struct AppreciationView view
	PUSH_VAR view
	PUSH_VAL Integer 0i
	PUSH_VAL Integer 0i
	PUSH_VAL Integer -1i
	CALL_METHOD create
	NOP
	PUSH_VAR view
	CALL_METHOD run
	NOP
	PUSH_VAR view
	CALL_METHOD destroy
	NOP
	CALL LeaveRecollectMode
	NOP
	PUSH_VAL Integer 0i
	Return
	@@Initialize

	Return this
EndFunc

