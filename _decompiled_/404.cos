Function AppreciationView::hide(Reference this, Integer fWait)
	PUSH_FIELD sprBase
	PUSH_VAL Integer 256i
	CALL_METHOD SetBlendingEnvelope
	NOP
	PUSH_FIELD sprBase
	PUSH_VAL Integer 500i
	CALL_METHOD BeginActivation
	NOP
	PUSH_VAR fWait
	PUSH_VAL Integer -1i
	==
	JUMP_OK _l_16
	PUSH_FIELD sprBase
	PUSH_VAL Integer -1i
	CALL WaitUntilSpriteActive
	NOP
@_l_16:
	PUSH_VAL Integer 0i
	Return
EndFunc

