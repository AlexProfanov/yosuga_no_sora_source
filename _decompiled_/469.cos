Function CheckCg(Reference src, String filename, Integer baseFlag, Integer flag)
	PUSH_VAR src
	PUSH_VAR filename
	==
	JUMP_OK _l_16
	PUSH_VAR baseFlag
	PUSH_VAL Integer 0i
	!=
	JUMP_OK _l_14
	PUSH_VAR baseFlag
	CALL SetCgFlag
	NOP
	PUSH_VAR flag
	CALL SetCgFlag
	NOP
@_l_14:
	PUSH_VAL Integer -1i
	Return
@_l_16:
	PUSH_VAL Integer 0i
	Return
	PUSH_VAL Integer 0i
	Return
EndFunc

