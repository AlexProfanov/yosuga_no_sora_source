Function CreateDateString(Reference time)
	INIT_LOCAL String ret
	INIT_LOCAL String temp
	PUSH_VAR ret
	PUSH_VAL String ""
	POP =
	NOP
	PUSH_VAR temp
	PUSH_VAR time
	GET_FIELD nYear
	POP =
	NOP
	PUSH_VAR ret
	PUSH_VAR temp
	PUSH_VAL String "/"
	+
	POP +=
	NOP
	PUSH_VAR time
	GET_FIELD nMonth
	PUSH_VAL Integer 10i
	<=
	JUMP_OK _l_26
	PUSH_VAR ret
	PUSH_VAL String "0"
	POP +=
	NOP
@_l_26:
	PUSH_VAR temp
	PUSH_VAR time
	GET_FIELD nMonth
	POP =
	NOP
	PUSH_VAR ret
	PUSH_VAR temp
	PUSH_VAL String "/"
	+
	POP +=
	NOP
	PUSH_VAR time
	GET_FIELD nDay
	PUSH_VAL Integer 10i
	<=
	JUMP_OK _l_46
	PUSH_VAR ret
	PUSH_VAL String "0"
	POP +=
	NOP
@_l_46:
	PUSH_VAR ret
	PUSH_VAL String ""
	PUSH_VAR time
	GET_FIELD nDay
	POP =
	PUSH_VAL String " "
	+
	POP +=
	NOP
	PUSH_VAR time
	GET_FIELD nHour
	PUSH_VAL Integer 10i
	<=
	JUMP_OK _l_64
	PUSH_VAR ret
	PUSH_VAL String "0"
	POP +=
	NOP
@_l_64:
	PUSH_VAR temp
	PUSH_VAR time
	GET_FIELD nHour
	POP =
	NOP
	PUSH_VAR ret
	PUSH_VAR temp
	PUSH_VAL String ":"
	+
	POP +=
	NOP
	PUSH_VAR time
	GET_FIELD nMinute
	PUSH_VAL Integer 10i
	<=
	JUMP_OK _l_84
	PUSH_VAR ret
	PUSH_VAL String "0"
	POP +=
	NOP
@_l_84:
	PUSH_VAR temp
	PUSH_VAR time
	GET_FIELD nMinute
	POP =
	NOP
	PUSH_VAR ret
	PUSH_VAR temp
	PUSH_VAL String ":"
	+
	POP +=
	NOP
	PUSH_VAR time
	GET_FIELD nSecond
	PUSH_VAL Integer 10i
	<=
	JUMP_OK _l_104
	PUSH_VAR ret
	PUSH_VAL String "0"
	POP +=
	NOP
@_l_104:
	PUSH_VAR temp
	PUSH_VAR time
	GET_FIELD nSecond
	POP =
	NOP
	PUSH_VAR ret
	PUSH_VAR temp
	POP +=
	NOP
	PUSH_VAR ret
	Return
	PUSH_VAL Integer 0i
	Return
EndFunc

