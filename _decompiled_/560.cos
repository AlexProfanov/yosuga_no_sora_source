Function IRect(Integer left, Integer top, Integer right, Integer bottom)
	INIT_LOCAL struct Rect rect
	PUSH_VAR rect
	GET_FIELD left
	PUSH_VAR left
	POP =
	NOP
	PUSH_VAR rect
	GET_FIELD top
	PUSH_VAR top
	POP =
	NOP
	PUSH_VAR rect
	GET_FIELD right
	PUSH_VAR right
	POP =
	NOP
	PUSH_VAR rect
	GET_FIELD bottom
	PUSH_VAR bottom
	POP =
	NOP
	PUSH_VAR rect
	Return
	PUSH_VAL Integer 0i
	Return
EndFunc

