Function LoadSystemData()
	PUSH_VAL struct File struct
	PUSH_VAL String "system.dat"
	CALL_METHOD IsExisting
	JUMP_OK _l_9
	PUSH_VAL String "system.dat"
	PUSH_OBJ  6
	CALL LoadObject
	NOP
	JUMP _l_13
@_l_9:
	CALL InitSystemData
	NOP
	CALL SaveSystemData
	NOP
@_l_13:
	PUSH_VAL Integer 0i
	Return
EndFunc

