Function MessageFrame::skip(Reference this, Integer f)
	PUSH_VAR f
	PUSH_VAL Integer -1i
	==
	JUMP_OK _l_10
	PUSH_FIELD sprMenu
	PUSH_VAL Integer 0i
	PUSH_VAL String "ID_SKIP"
	CALL_METHOD Enable
	NOP
	JUMP _l_15
@_l_10:
	PUSH_FIELD sprMenu
	PUSH_VAL Integer 1i
	PUSH_VAL String "ID_SKIP"
	CALL_METHOD Enable
	NOP
@_l_15:
	PUSH_VAL Integer 0i
	Return
EndFunc

