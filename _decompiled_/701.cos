Function Scroll(Integer x, Integer y, Integer time, Integer accel, Integer fWait)
	CALL IsLoad
	PUSH_VAL Integer -1i
	==
	JUMP_OK _l_6
	PUSH_VAL Integer 0i
	Return
@_l_6:
	PUSH_OBJ  10
	PUSH_VAR x
	PUSH_VAR y
	PUSH_VAR time
	PUSH_VAR accel
	PUSH_VAR fWait
	CALL_METHOD scroll
	NOP
	PUSH_VAL Integer 0i
	Return
EndFunc

