Function SetTimezomeBustup(Reference str, Integer timezone)
	PUSH_VAR timezone
	PUSH_VAL Integer 1i
	==
	JUMP_OK _l_9
	PUSH_VAR str
	PUSH_VAL String "D"
	POP +=
	NOP
	JUMP _l_26
@_l_9:
	PUSH_VAR timezone
	PUSH_VAL Integer 2i
	==
	JUMP_OK _l_18
	PUSH_VAR str
	PUSH_VAL String "E"
	POP +=
	NOP
	JUMP _l_26
@_l_18:
	PUSH_VAR timezone
	PUSH_VAL Integer 3i
	==
	JUMP_OK _l_26
	PUSH_VAR str
	PUSH_VAL String "N"
	POP +=
	NOP
@_l_26:
	PUSH_VAL Integer 0i
	Return
EndFunc

