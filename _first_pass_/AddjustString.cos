Function AddjustString(String src, Integer num)
	Integer length
	length := src.GetLength()
	If (length < num)
		Return src
	EndIf
	String temp
	Integer i
	i := 0
	While (i + num <= length)
		temp += src.Middle(i, num) + "\n"
		i += num
	EndWhile
	If (i <= length)
		temp += src.Middle(i, num)
	EndIf
	Return temp
EndFunc

