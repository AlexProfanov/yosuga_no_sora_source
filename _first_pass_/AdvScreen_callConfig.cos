Function AdvScreen::callConfig()
	this.msgFrame.enable(0)
	ConfigWindow win
	win.create()
	win.show()
	If (isSelect() == -1)
        transparencySelect(256)
    EndIf
	Integer ret
	ret := win.run()
	If (isSelect() == -1)
        transparencySelect(0)
    EndIf
	win.hide(-1)
	win.destroy()
	this.msgFrame.enable(-1)
	Return ret
EndFunc

