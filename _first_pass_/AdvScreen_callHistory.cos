Function AdvScreen::callHistory()
	If ({PUSH_OBJ  4}.num() < 1) ;//TODO
        Return 0
    EndIf
    If (ifSelect() == -1)
        this.transparencySelect(256)
	Else
        msgFrame.hideBlink()
    EndIf
	HistoryWindow win
	win.create()
	win.show()
	hideMessage()
	win.run()
	if (isSelect() == -1)
        this.transparencySelect(0)
    Else
        msgFrame.showBlink()
    EndIf
	showMessage
	win.hide(-1)
	win.destroy()
EndFunc

