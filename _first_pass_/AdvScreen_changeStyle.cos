Function AdvScreen::changeStyle(Integer style)
	If (this.fCreate == 0)
        Return
    Else
	{PUSH_UNK}.Lock()  ; //TODO
	this.msgFrame.changeStyle(style)
	If (isSelect() == -1)
        this.msgFrame.output("", "\n", -1)
        setupSelectItem()
        transparencySelect(0, -1)
    EndIf
	{PUSH_UNK}.Unlock()  ; //TODO
EndFunc

