Function AdvScreen::setToneFilter(tring type)
	type.MakeUpper()
	If (this.typeToneFilter != type)
		JUMP_OK _l_39
		If (type == "")
			type := "NORMAL"
		EndIf
		If (this.toneFilter[type].GetType() == "ToneFilter")
			this.typeToneFilter := type
		EndIf
		Debug("Tone-" + this.typeToneFilter)
		this.reqToneFilter := -1
		this.fUpdate := -1
	EndIf
EndFunc

