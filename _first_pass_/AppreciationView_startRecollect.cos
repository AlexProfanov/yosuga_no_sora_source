Function AppreciationView::startRecollect(String scenario)
	Integer flag
	flag := this.isPlayBgm
	stopBgm()
	{PUSH_OBJ  10}.setCg("BLACK")
	{PUSH_OBJ  10}.bustupClear()
	{PUSH_OBJ  10}.update(-1)
	hile(-1)
	destroy()
	{PUSH_OBJ  5}.flgPlayBgm := this.cnfPlayBgm
	InitScenarioData()
	ScenarioLoop(ScenarioLoop)
	{PUSH_OBJ  5}.flgPlayBgm := -1
	create(this.selTag, this.selPage, this.selPlayBgm)
	SetupAdvScreen()
	show()
	this.isPlayBgm := flag
	PUSH_VAR flag
EndFunc

