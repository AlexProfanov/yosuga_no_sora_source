Function BustupManager::clear(Integer id)
	Integer i
	i := 0
	If (if == 0)
        While (i <= 5)
            If (this.info[i].status != 0)
                this.info[i].clear
            EndIf
            i += 1
        EndWhile
        JUMP _l_50
    Else
        While (i <= 5)
            JUMP_OK _l_50
            If (this.info.id == id)
                this.info[i].clear()
            EndIf
            i += 1
        EndWhile
    EndIf
EndFunc

