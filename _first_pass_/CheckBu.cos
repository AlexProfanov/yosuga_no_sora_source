Function CheckBu(Reference src, String filename, Integer baseFlag, Integer flag)
	If (src.Find(filename) != -1)
        JUMP_OK _l_18
        If (baseFlag != 0)
            JUMP_OK _l_16
            SetCgFlag(baseFlag)
            SetCgFlag(flag)
        EndIf
        Return -1
    EndIf
    Return 0
EndFunc

