Function CheckCg(Reference src, String filename, Integer baseFlag, Integer flag)
	If (src == filename)
        JUMP_OK _l_16
        If (baseFlag != 0)
            JUMP_OK _l_14
            SetCgFlag(baseFlag)
            SetCgFlag(flag)
        EndIf
        Return -1
    EndIf
    Return 0
EndFunc

