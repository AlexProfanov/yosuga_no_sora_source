Function ConfigWindow::apply()
	If ({PUSH_OBJ  5}.screenType == 0)
		(this.sprBase.CheckButton("ID_WINDOW", -1)
	Else
		this.sprBase.CheckButton("ID_FULLSCREEN", -1)
	EndIf
	this.sprBase.CheckButton("ID_CHECK_BGM", {PUSH_OBJ  5}.flgPlayBgm)
	this.sprBase.SetHorzScrollPos(256 * {PUSH_OBJ  5}.volBgm, "ID_VOL_BGM")
	this.sprBase.CheckButton("ID_CHECK_BGM", {PUSH_OBJ  5}.flgPlaySe)
	this.sprBase.SetHorzScrollPos(256 * {PUSH_OBJ  5}.volSe, "ID_VOL_SE")
	this.sprBase.CheckButton("ID_CHECK_SYSTEM", {PUSH_OBJ  5}.flgPlaySysSe)
	this.sprBase.SetHorzScrollPos(256 * {PUSH_OBJ  5}.volSysSe, "ID_VOL_SYSTEM")
	this.sprBase.CheckButton("ID_CHECK_VOICE", {PUSH_OBJ  5}.flgPlayVoice)
	this.sprBase.SetHorzScrollPos(256 * {PUSH_OBJ  5}.volVoice, "ID_VOL_VOICE")
	If ({PUSH_OBJ  5}.screenEffect == 0)
		this.sprBase.CheckButton("ID_NORMAL", -1)
	ElseIf ({PUSH_OBJ  5}.screenEffect == 1)
		this.sprBase.CheckButton("ID_NONE", -1)
	EndIf
	this.sprBase.SetHorzScrollPos(256 - {PUSH_OBJ  5}.windowDepth, "ID_WINDOW_DEPTH")
	{PUSH_OBJ  10}.msgFrame.transparencyBase({PUSH_OBJ  5}.windowDepth)
	this.sprBase.SetHorzScrollPos({PUSH_OBJ  5}.messageSpeed, "ID_MESSAGE_SPEED")
	If ({PUSH_OBJ  5}.readSkip == -1)
		this.sprBase.CheckButton("ID_READED", -1)
	Else
		this.sprBase.CheckButton("ID_ALL", -1)
	EndIf
	If ({PUSH_OBJ  5}.voiceStopOnClick == -1)
		this.sprBase.CheckButton("ID_CLICK_STOP", -1)
	Else
		this.sprBase.CheckButton("ID_CLICK_PLAY", -1)
	EndIf
	If ({PUSH_OBJ  5}.lockSkip == -1)
		this.sprBase.CheckButton("ID_STATIC", -1)
	Else
		this.sprBase.CheckButton("ID_REMOVE", -1)
	EndIf
	this.sprBase.SetHorzScrollPos({PUSH_OBJ  5}.automodeSpeed, "ID_AUTOMODE_SPEED")
EndFunc

