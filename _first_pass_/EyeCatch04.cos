Function EyeCatch04(Integer fEnter)
	Reference spr
	spr := {PUSH_OBJ  25}
	If (fEnter == -1)
		SpriteParam param
		{PUSH_OBJ  10}.sprAdvBase.GetParameter(param)
		{PUSH_OBJ  10}.sprAdvBase.SetParameter(param)
		spr["Logo"] := Sprite
		spr["Logo"].LoadImage("title.eri")
		spr["Logo"].MovePosition(430, 510)
		spr["Logo"].SetTransparency(256)
		spr["Logo"].SetVisible(-1)
		{PUSH_UNK}.AddSprite(50800, spr["Logo"])
		Bezier2D bzMag
		bzMag.SetLinear(1.0, 1.0, 0.0, 0.0)
		bzMag.SetAcceleration(3.0, 0.0)
		{PUSH_OBJ  10}.sprAdvBase.SetBezierCurve(ref, ref, bzMag)
		{PUSH_OBJ  10}.sprAdvBase.SetBlendingEnvelope(256)
		{PUSH_OBJ  10}.sprAdvBase.BeginActivation(1000)
		{PUSH_OBJ  10}.sprAdvBase.WaitUntilSpriteActive(-1)
		spr["Logo"].SetBlendingEnvelope(0)
		spr["Logo"].BeginActivation(1500)
		spr["Logo"].WaitUntilSpriteActive(-1)
		ScWait(3000)
	Else
		SpriteParam param
		{PUSH_OBJ  10}.sprAdvBase.GetParameter(param)
		param.ptDstPos := IPoint(0, 0)
		param.ptRevCenter := IPoint(0, 0)
		param.rVertUnit := 1.0
		param.rHorzUnit := 1.0
		{PUSH_OBJ  10}.sprAdvBase.SetParameter(param)
		{PUSH_OBJ  10}.sprAdvBase.SetBlendingEnvelope(0)
		{PUSH_OBJ  10}.sprAdvBase.BeginActivation(1000)
		spr["Logo"].SetBlendingEnvelope(256)
		spr["Logo"].BeginActivation(1000)
		spr["Logo"].WaitUntilSpriteActive(-1)
	EndIf
EndFunc

