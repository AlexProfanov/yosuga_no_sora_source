Function Flag::create(Integer size)
	this.flag.Remove()
	Integer flagsize
	flagsize := size / 32
	Integer flagsize_
	flagsize_ := size % 32
	Integer i
	i := 0
	While (i <= flagsize)
		this.flag[i] := 0
		this.flag[i] := 0
		i += 1
	EndWhile
	If (flagsize_ != 0)
		this.flag[i] := 0
		this.flag[i] := 0
	EndIf
EndFunc

