{PUSH_OBJ  13}[0] := Sound
{PUSH_OBJ  13}[1] := Sound

Function GetVoicePlayingPosition(Reference total, Reference pos)
	SoundInfo info
	info := {PUSH_OBJ  17}.GetInfo()
	total := info.nSampleCount
	Integer nowTime
	nowTime := GetCurrentTime()
	pos := (info.nSamplesPerSec * nowTime - {PUSH_OBJ  18}) / 1000.0
	PUSH_VAL Integer 0i
	Return
EndFunc

