Function HistoryWindow::show()
	If ({PUSH_OBJ  5}.screenEffect == 0)
		ezier2D bzMag
		bzMag.SetLinear(1.0, 0.0, 1.0, 1.0)
		bzMag.SetAcceleration(3.0, 0.0)
		sprBase.SetBezierCurve(ref, ref, bzMag)
		sprBase.SetBlendingEnvelope(0)
		sprBase.BeginActivation(300)
	Else
		sprBase.SetTransparency(0)
	EndIf
EndFunc

