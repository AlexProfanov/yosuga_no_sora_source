Function MessageFrame::destroy()
	this.isCreate := 0
	sprFrame.SetVisible(0)
	sprBase[0].SetVisible(0)
	sprBase[1].SetVisible(0)
	sprMenu.SetVisible(0)
	sprClose.SetVisible(0)
	msprName.SetVisible(0)
	msprMess.SetVisible(0)
	sprParent.DetachAllSprite()
	sprParent.Release()
	sprBase.Remove()
	sprFrame.Release()
	sprMenu.Release()
	sprClose.Release()
	msprName.Release()
	msprMess.Release()
	sprBlinkHit.Release()
EndFunc

